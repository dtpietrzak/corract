{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "jsxImportSource": "corract",
    "declaration": true,
    "outDir": "./dist",
    "rootDir": "src",
    "skipLibCheck": true,
    "baseUrl": "./src",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
  },
  "include": [
    "./"
  ],
  "exclude": [
    "./dist"
  ]
}