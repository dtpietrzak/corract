{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "corract",
    "declaration": true,
    "moduleResolution": "bundler",
    "outDir": "./dist",
    "rootDir": "src",
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@types/*": [
        "_types/*"
      ],
    }
  },
  "include": [
    "./"
  ],
  "exclude": [
    "./dist"
  ]
}