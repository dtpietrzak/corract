{
  "name": "corract-monorepo",
  "version": "0.0.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "corract-monorepo",
      "version": "0.0.0",
      "workspaces": [
        "apps/*",
        "apps/examples/*",
        "packages/*"
      ],
      "devDependencies": {
        "turbo": "^2.5.4",
        "typescript": "5.8.2"
      },
      "engines": {
        "node": ">=18"
      }
    },
    "apps/basic-app": {
      "version": "0.0.0",
      "dependencies": {
        "corract": "*"
      }
    },
    "apps/examples/basic-app": {
      "version": "0.0.0",
      "extraneous": true,
      "dependencies": {
        "corract": "*"
      }
    },
    "node_modules/basic-app": {
      "resolved": "apps/basic-app",
      "link": true
    },
    "node_modules/corract": {
      "resolved": "packages/corract",
      "link": true
    },
    "node_modules/create-corract-app": {
      "resolved": "packages/create-corract-app",
      "link": true
    },
    "node_modules/turbo": {
      "version": "2.5.4",
      "dev": true,
      "license": "MIT",
      "bin": {
        "turbo": "bin/turbo"
      },
      "optionalDependencies": {
        "turbo-darwin-64": "2.5.4",
        "turbo-darwin-arm64": "2.5.4",
        "turbo-linux-64": "2.5.4",
        "turbo-linux-arm64": "2.5.4",
        "turbo-windows-64": "2.5.4",
        "turbo-windows-arm64": "2.5.4"
      }
    },
    "node_modules/turbo-darwin-arm64": {
      "version": "2.5.4",
      "cpu": [
        "arm64"
      ],
      "dev": true,
      "license": "MIT",
      "optional": true,
      "os": [
        "darwin"
      ]
    },
    "node_modules/typescript": {
      "version": "5.8.2",
      "dev": true,
      "license": "Apache-2.0",
      "bin": {
        "tsc": "bin/tsc",
        "tsserver": "bin/tsserver"
      },
      "engines": {
        "node": ">=14.17"
      }
    },
    "packages/corract": {
      "version": "0.1.0",
      "bin": {
        "corract": "bin/corract.js"
      }
    },
    "packages/create-corract-app": {
      "version": "0.1.0",
      "dependencies": {
        "corract": "*"
      },
      "bin": {
        "create-corract-app": "index.js"
      }
    }
  }
}
